{
  "name": "mentor-humanista",
  "version": "1.0.0",
  "private": true,
  "description": "Mentor Humanista - Capacitor app (Android & iOS)",
  "scripts": {
    "cap:init": "cap init 'Mentor Humanista' com.saberconsciente.mentorhumanista --web-dir=dist",
